<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.projetopdm">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo2"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo2"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProjetoPDM">
        <activity
            android:name=".Splash"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Login"/>
        <activity android:name=".Registration"/>
        <activity android:name=".Trips"/>
        <activity android:name=".Trip"/>
        <activity android:name=".Fuel"/>
        <activity android:name=".Airfare"/>
        <activity android:name=".Snack"/>
        <activity android:name=".Accommodation"/>
        <activity android:name=".Entertainment"/>
        <activity android:name=".InitialInformation"/>
        <activity android:name=".CardInfo"/>
    </application>

</manifest>